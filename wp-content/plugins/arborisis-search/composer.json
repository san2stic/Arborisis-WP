{
    "name": "arborisis/search",
    "description": "OpenSearch integration plugin for Arborisis",
    "type": "wordpress-plugin",
    "require": {
        "php": ">=7.4",
        "nyholm/psr7": "^1.8",
        "opensearch-project/opensearch-php": "^2.0",
        "symfony/http-client": "^8.0"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
